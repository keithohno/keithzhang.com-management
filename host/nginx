server {
    listen      80;
    server_name _;
    charset utf-8;
    root    /home/keith/keithzhang.com/dist;
    index   index.html index.htm;
    # Always serve index.html for any request
    location / {
        root /home/keith/keithzhang.com/dist;
        try_files $uri /index.html;
    }
    error_log  /var/log/nginx/error.log;
    access_log /var/log/nginx/access.log;
}
